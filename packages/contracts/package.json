    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "chai": "^4.3.7",
    "ethers": "^5.7.2",
    "hardhat": "^2.13.1",
    "hardhat-deploy": "^0.11.26",
    "hardhat-gas-reporter": "^1.0.9",
    "ipfs-http-client": "^51.0.0",
    "mocha": "^10.1.0",
    "solhint": "^3.4.0",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "tmp-promise": "^3.0.3"
  },
  "dependencies": {
    "@aragon/osx": "^1.2.0",
    "@aragon/osx-ethers": "^1.2.1",
    "@ensdomains/ens-contracts": "0.0.20",
    "@openzeppelin/contracts": "^4.8.2",
    "@openzeppelin/contracts-upgradeable": "^4.8.2",
    "@openzeppelin/hardhat-upgrades": "^1.23.0"
  },
  "files": [
    "/src"
  ],
  "keywords": [
    "blockchain",
    "ethers",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity",
    "template",
    "typescript",
    "typechain"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && yarn typechain",
    "deploy": "hardhat deploy",
    "lint": "yarn lint:sol && yarn lint:ts",
    "lint:sol": "solhint --max-warnings 0 \"src/**/*.sol\"",
    "lint:ts": "eslint --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "DOTENV_CONFIG_PATH=./.env.example yarn typechain",
    "test": "hardhat test",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "clean": "rimraf ./artifacts ./cache ./coverage ./types ./coverage.json && yarn typechain"
  }
}
